@prefix sp: <http://spinrdf.org/sp#>.
@prefix spin: <http://spinrdf.org/spin#>.

[ a       sp:Select ;
	sp:text """
SELECT (COUNT(?object) AS ?count)
WHERE {
	?this ?arg1 ?object
}
""";
  sp:resultVariables (_:count [ a       sp:Count ;
                        sp:expression _:object;
						sp:varName "count"
                      ]) ;
  sp:where ([ sp:object _:object ;
              sp:predicate _:arg1 ;
              sp:subject spin:_this
            ])
].

_:arg1 sp:varName "arg1".
_:object sp:varName "object".
